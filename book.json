{
    "title": "ismumu's blog",
    "description": "ismumu's blog",
    "author": "ismumu",
    "links": {
        "sidebar" : {
            "Ismumu's Home" : "http://ismumu.com"
        }
    },
    "styles": {
        "website": "public/css/common.css"
    },
    "plugins": [
        "favicon",
        "tbfed-pagefooter@^0.0.1",
        "github",
        "disqus"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "ismumu"
        },
        "github": {
            "url": "https://github.com/ismumu"
        },
        "sharing": {
            "twitter": false,
            "facebook": false,
            "all": []
        },
        "favicon": {
            "shortcut": "favicon.ico",
            "bookmark": "favicon.ico"
        },
        "tbfed-pagefooter": {
            "modify_label": "时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}